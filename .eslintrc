{
  "env": {
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-underscore-dangle": "off",
    "no-console": "error",
    "no-multi-spaces": "error",
    "class-methods-use-this": "off"
  }
}
